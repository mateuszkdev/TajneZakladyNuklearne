{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "lib": ["es6", "dom"],
      "outDir": "./build",
      "strict": false,
      "moduleResolution": "node",
      "baseUrl": "./src",
      "typeRoots": ["node_modules/@types"],
      "esModuleInterop": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "resolveJsonModule": true,
      "incremental": true,
    }
}